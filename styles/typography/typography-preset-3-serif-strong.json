{
	"version": 3,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"title": "Serif Strong",
	"slug": "typography-preset-3s",
	"settings": {
		"typography": {
			"fontFamilies": [
				{
					"name": "sans-serif (Helvetica Neue, Helvetica, Arial)",
					"slug": "helvetica-arial",
					"fontFamily": "\"Helvetica Neue\",Helvetica,Arial,sans-serif"
				},
				{
					"name": "sans-serif (Geneva, Tahoma, Verdana)",
					"slug": "geneva-tahoma-verdana",
					"fontFamily": "Geneva,Tahoma,Verdana,sans-serif"
				},
				{
					"name": "sans-serif system (BlinkMacSystemFont, Roboto, Segoe UI, Ubuntu)",
					"slug": "system",
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,sans-serif"
				},
				{
					"name": "serif (Cambria, Georgia)",
					"slug": "cambria-georgia",
					"fontFamily": "Cambria,Georgia,serif"
				},
				{
					"name": "serif (Hoefler Text, Baskerville Old Face, Garamond, Times New Roman",
					"slug": "hoefler-times-new-roman",
					"fontFamily": "\"Hoefler Text\",\"Baskerville Old Face\",Garamond,\"Times New Roman\",serif"
				},
				{
					"name": "monospace (Courier New - Courier)",
					"slug": "monospace",
					"fontFamily": "\"Courier New\",Courier,monospace"
				}
			]
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var:preset|font-family|cambria-georgia"
		},
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontWeight": "1000",
					"letterSpacing": "2px"
				}
			}
		},
		"elements": {
			"heading": {
				"typography": {
					"fontWeight": "1000",
					"letterSpacing": "2px"
				}
			},
			"button": {
				"typography": {
					"letterSpacing": "0.9px"
				}
			}
		}
	}
}
