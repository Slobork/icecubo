{
	"version": 3,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"title": "Oval",
	"slug": "icecubo-oval-post-byline",
	"blockTypes": ["core/post-author", "core/post-date", "core/post-navigation-link", "core/post-terms", "core/query-pagination" ],
	"styles": {
		"color": {			
			"background": "var(--wp--preset--color--white-ice)",
			"text": "var(--wp--preset--color--darko-down-bottom)"
		},
		"spacing": {
			"padding": {
				"left": "var(--wp--custom--spacing-s)",
				"right": "var(--wp--custom--spacing-s)"
			}
		},
		"border": {
			"radius": "var(--wp--custom--radius-middle)"
		}
	}
}